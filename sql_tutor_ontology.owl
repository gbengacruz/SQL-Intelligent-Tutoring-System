        <suggestionType>SUGGESTION</suggestionType>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sqltutor/ontology#index_usage_suggestion">
        <rdf:type rdf:resource="http://www.semanticweb.org/sqltutor/ontology#Suggestion"/>
        <suggestionMessage>ðŸ’¡ Good practice: Filter on indexed columns (like primary keys) in WHERE clause for better query performance</suggestionMessage>
        <suggestionType>SUGGESTION</suggestionType>
    </owl:NamedIndividual>
    
    
