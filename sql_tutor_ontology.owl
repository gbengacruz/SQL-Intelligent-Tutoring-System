        <suggestionType>SUGGESTION</suggestionType>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sqltutor/ontology#table_alias_suggestion">
        <rdf:type rdf:resource="http://www.semanticweb.org/sqltutor/ontology#Suggestion"/>
